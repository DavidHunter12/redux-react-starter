{
  "name": "redux-react-starter",
  "version": "1.0.0",
  "description": "A TypeScript starter project for Redux and React applications.",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" \"npm run rest\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "rest": "json-server --config json-server.json db.json"
  },
  "keywords": [
    "redux",
    "react"
  ],
  "author": "Eric W. Greene",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^2.2.0",
    "json-server": "^0.8.21",
    "lite-server": "^2.2.2",
    "typescript": "^2.0.3",
    "typings": "^1.4.0"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "redux": "^3.6.0",
    "systemjs": "^0.19.38"
  }
}
